{
  "name": "soccer",
  "version": "1.0.0",
  "description": "soccer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coranos/soccer.git"
  },
  "scripts": {
    "eslint": "eslint --fix 'scripts/**/*.js'",
    "test": "./node_modules/mocha/bin/mocha --recursive -t 100000 test/unit",
    "coverage": "nyc -x 'test/unit/**' -x 'test/util/**' ./node_modules/mocha/bin/mocha --recursive -t 100000 test/unit",
    "start": "node -max-old-space-size=1700 scripts/index.js",
    "screenstart": "screen -dmSL soccer npm start;screen -x soccer",
    "screenrestart": "npm stop;screen -x soccer;screen -dmSL soccer npm start;screen -x soccer",
    "stop": "node scripts/stop.js"
  },
  "author": "Coranos",
  "license": "MIT",
  "dependencies": {
    "blakejs": "^1.1.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-handlebars": "^5.1.0",
    "mkdirp": "^1.0.4",
    "request": "^2.88.2",
    "socket.io": "^2.3.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  }
}
